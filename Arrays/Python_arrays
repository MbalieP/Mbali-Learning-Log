# Single-dimensional array
numbers = [1, 2, 3, 4, 5]
print("Python 1D Array:", numbers)

# Access elements
print("First element:", numbers[0])


# Multi-dimensional array (2D list)
matrix = [
    [1, 2, 3],
    [4, 5, 6]
]
print("Python 2D Array:", matrix)
print("Element at [1][2]:", matrix[1][2])

